<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Intel MKL 11 x86 threaded static"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(INTEL_MKL)\mkl\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="mkl_intel_c.lib mkl_core.lib mkl_intel_thread.lib libiomp5md.lib"
		AdditionalLibraryDirectories="$(INTEL_MKL)\mkl\lib\ia32;$(INTEL_MKL)\compiler\lib\ia32"
	/>
</VisualStudioPropertySheet>
